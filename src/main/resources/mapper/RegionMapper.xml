<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qualco.demo.mapper.RegionMapper">

    <select id="getAllRegions" resultType="com.qualco.demo.model.Region">
        SELECT region_id, name, continent_id
        FROM regions
        ORDER BY name
    </select>

</mapper>